 {

    "manifest_version": 3,
    "name": "Proxy",
    "description": "Proxy extension for Google Chrome",
    "version": "1.1",
    "background": {
      "service_worker":"background.js"
    },
    "permissions": [
        "proxy",
        "storage"
    ],
    "host_permissions": [
      "https://www.google.com/",
      "https://ipapi.co*"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }